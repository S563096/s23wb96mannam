extends layout

block content
  h1= title
  - var image_names = ["perf1.jpg","perf2.jpg","perf3.jpg","perf4.jpg","perf5.jpg"]
  - for(let i=0;i<3;i++){
     - let random= Math.floor(Math.random()*5)
     img(src=`/images/${image_names[random]}` alt=`Image No ${random}` height="300px" width="300px")
  - }